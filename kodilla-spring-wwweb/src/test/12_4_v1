SELECT T.SUMMARY, L.NAME
FROM
ISSUESS T
JOIN ISSUESLIST L ON  T.ISSUESLIST_ID=L.ID

SELECT T.SUMMARY, U.FIRSTNAME, U.LASTNAME
FROM
ISSUESS T
JOIN USERS U ON  T.USER_ID_ASSIGNEDTO=U.ID


SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) TOTAL_ISSUES
FROM ISSUESS T
JOIN USERS U ON  T.USER_ID_ASSIGNEDTO=U.ID
GROUP BY T.USER_ID_ASSIGNEDTO
HAVING COUNT(*) > 0;