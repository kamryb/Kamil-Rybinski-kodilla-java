CREATE TABLE ISSUESLIST
(
	ID SERIAL PRIMARY KEY,
	NAME VARCHAR(100)
);


CREATE TABLE ISSUES
(
	ID SERIAL PRIMARY KEY,
	ISSUESLIST_ID FOREIGN KEY(ID) REFERENCES ISSUESLIST(ID),
	SUMMARY - VARCHAR(100),
	DESCRIPTION - VARCHAR(100),
	USER_ID_ASSIGNEDTO FOREIGN KEY(ISSUELIST_ID) REFERENCES USER(ID)
);

